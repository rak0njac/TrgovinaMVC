                @Html.ValidationMessageFor(model => model.kolicina, "", new { @class = "text-danger", @data_orgname = "kolicina" })
            </div>
        </div>
    </td>
    <td>
        <div>
            <div>
                <input type="button" value="Obriši" class="btn btn-primary" onclick="DeleteStavka(this)" @*style="display:none"*@ />
            </div>
        </div>
    </td>
</tr>
