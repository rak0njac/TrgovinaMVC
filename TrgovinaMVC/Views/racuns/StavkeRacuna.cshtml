    <td>
        <div>
            <div>
                @*<input type="hidden" name="artikal.naziv.Index" value="" />*@
                @Html.EditorFor(model => model.artikal.naziv, new { htmlAttributes = new { @class = "form-control", @data_orgname = "artikal.naziv" } })
